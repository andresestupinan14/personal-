#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
	int N;
	
	printf("Escriba su nota: ");
	scanf("%d",&N);
	
	if (N>0&&N<5){
		printf("Suspenso");
		
	}else if (4<N && N<7){
		printf("Aprobado");
		
	}else if (6<N && N<9){
		printf("Notable");
		
	}else if (9==N){
		printf("Sobresaliente");
		
	}else if (N==10){
		printf("Matricula de honor");
		
	}else {
		printf("La nota ingresada no es valida");
	}
			
	return 0;
}
